if (typeof STATUS === "undefined" || STATUS === null || STATUS === "") {
    return;
}
var formattedText = STATUS;

if (STATUS === "Rejected") {
    formattedText = "Error";
}
if (STATUS === "Pending") {
    formattedText = "Warning";
}
if (STATUS === "Approved") {
    formattedText = "Success";
}

return formattedText;
