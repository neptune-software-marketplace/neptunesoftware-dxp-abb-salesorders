if (typeof STATUS === "undefined" || STATUS === null || STATUS === "") { return; }  
var formattedText = STATUS; 

if(STATUS === "Approved"){
    formattedText = "sap-icon://accept";
}
if(STATUS === "Rejected"){
    formattedText = "sap-icon://decline"
}
if(STATUS === "Pending"){
    formattedText = "sap-icon://pending"
}
 
return formattedText;
